<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/back"
    android:focusable="false"
    android:layoutMode="opticalBounds"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".MainActivity" >

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView1"
        android:layout_marginTop="15dp"
        android:src="@drawable/garlicresto" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:text="Resto"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="30sp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/imageView1"
        android:layout_marginTop="15dp"
        android:src="@drawable/icecapucino" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView2"
        android:layout_below="@+id/textView2"
        android:text="Rp 10.000"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textSize="16sp" />

    <Button
        android:id="@+id/Pesan1"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="80dp"
        android:layout_height="35dp"
        android:layout_above="@+id/imageView3"
        android:layout_alignLeft="@+id/Pesan"
        android:layout_alignRight="@+id/Pesan"
        android:layout_below="@+id/Dpesanan1"
        android:text="Pesan" />

    <TextView
        android:id="@+id/Dpesanan2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/Dpesanan1"
        android:layout_alignTop="@+id/imageView3"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <Button
        android:id="@+id/Pesan2"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_alignBottom="@+id/negatif2"
        android:layout_alignLeft="@+id/Pesan1"
        android:layout_alignRight="@+id/Pesan1"
        android:layout_below="@+id/Dpesanan3"
        android:text="Pesan" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/imageView2"
        android:layout_marginTop="16dp"
        android:src="@drawable/jusapple" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/imageView3"
        android:layout_marginTop="15dp"
        android:src="@drawable/icestrawberry" />

    <TextView
        android:id="@+id/Dpesanan4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/Dpesanan3"
        android:layout_alignTop="@+id/imageView4"
        android:text="Juice Strawberry"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/Dpesanan5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/Dpesanan4"
        android:layout_below="@+id/Dpesanan4"
        android:text="Rp 10.000"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textSize="16sp" />

    <Button
        android:id="@+id/Pesan3"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="80dp"
        android:layout_height="35dp"
        android:layout_alignBottom="@+id/negatif3"
        android:layout_alignLeft="@+id/Pesan2"
        android:layout_alignRight="@+id/Pesan2"
        android:layout_below="@+id/Dpesanan5"
        android:text="Pesan" />

    <Button
        android:id="@+id/plus"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_above="@+id/imageView2"
        android:layout_toRightOf="@+id/textView1"
        android:text="+" />

    <Button
        android:id="@+id/negatif2"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_alignBottom="@+id/Angkaa2"
        android:layout_alignRight="@+id/Dpesanan4"
        android:layout_alignTop="@+id/plus2"
        android:text="-" />

    <Button
        android:id="@+id/negatif1"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_above="@+id/imageView3"
        android:layout_alignLeft="@+id/negatif2"
        android:layout_alignTop="@+id/plus1"
        android:text="-" />

    <TextView
        android:id="@+id/Angkaa1"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_above="@+id/imageView3"
        android:layout_alignRight="@+id/TextView03"
        android:layout_alignTop="@+id/negatif1"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/Angkaa2"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_alignBottom="@+id/plus2"
        android:layout_alignRight="@+id/TextView03"
        android:layout_alignTop="@+id/plus2"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/Angkaa3"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_alignBottom="@+id/negatif3"
        android:layout_alignLeft="@+id/Angkaa2"
        android:layout_alignTop="@+id/negatif3"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/textAngka"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageView2"
        android:layout_toRightOf="@+id/textView1"
        android:text="Ice Capucino"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/Dpesanan1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textAngka"
        android:layout_below="@+id/textAngka"
        android:text="Rp 11.000"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textSize="16sp" />

    <Button
        android:id="@+id/plus2"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_above="@+id/imageView4"
        android:layout_toRightOf="@+id/textView1"
        android:text="+" />

    <Button
        android:id="@+id/Pesan"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="80dp"
        android:layout_height="35dp"
        android:layout_alignBottom="@+id/plus"
        android:layout_alignLeft="@+id/Back"
        android:layout_alignTop="@+id/Angkaa"
        android:text="Pesan" />

    <Button
        android:id="@+id/negatif3"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_alignBaseline="@+id/plus3"
        android:layout_alignBottom="@+id/plus3"
        android:layout_alignLeft="@+id/negatif"
        android:text="-" />

    <TextView
        android:id="@+id/TextView03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView3"
        android:layout_alignTop="@+id/Dpesanan2"
        android:text="Juice Apple"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textSize="16sp" />

    <Button
        android:id="@+id/plus1"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_above="@+id/imageView3"
        android:layout_toRightOf="@+id/textView1"
        android:text="+" />

    <Button
        android:id="@+id/Daftar2"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="30dp"
        android:layout_above="@+id/imageView1"
        android:layout_alignRight="@+id/textView2"
        android:layout_alignTop="@+id/Back"
        android:text="Daftar" />

    <Button
        android:id="@+id/negatif"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_above="@+id/imageView2"
        android:layout_alignRight="@+id/textView2"
        android:text="-" />

    <Button
        android:id="@+id/plus3"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_alignBottom="@+id/imageView4"
        android:layout_alignLeft="@+id/TextView03"
        android:text="+" />

    <Button
        android:id="@+id/Back"
        style="?android:attr/buttonStyleSmall"
        android:layout_width="75dp"
        android:layout_height="30dp"
        android:layout_alignBottom="@+id/textView1"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/textView1"
        android:text="Back" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageView1"
        android:layout_toRightOf="@+id/textView1"
        android:text="Ice Blue Tropical"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/Angkaa"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_alignBottom="@+id/negatif"
        android:layout_toLeftOf="@+id/negatif"
        android:text="  "
        android:textSize="24sp" />

    <TextView
        android:id="@+id/Dpesanan3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/Dpesanan2"
        android:layout_below="@+id/TextView03"
        android:text="Rp 9.000"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textSize="16sp" />

</RelativeLayout>